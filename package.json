{
  "name": "pub_autotag",
  "version": "1.1.0",
  "description": "Automatically create a tag whenever the version changes in pubspec.yaml",
  "main": "dist/index.js",
  "directories": {
    "lib": "src"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.5",
    "@vercel/ncc": "^0.38.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@actions/core": "^2.0.1",
    "@actions/github": "^6.0.1",
    "js-yaml": "^4.1.1"
  },
  "scripts": {
    "build": "tsc && ncc build dist/main.js -o dist",
    "lint": "biome lint --write src",
    "format": "biome format --write src",
    "check": "biome check --write src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ricardodalarme/action_autotag"
  },
  "keywords": ["auto", "tag", "flutter", "autotag", "repo", "dart", "version"],
  "author": "Ricardo Dalarme",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ricardodalarme/action_autotag/issues"
  },
  "homepage": "https://github.com/ricardodalarme/action_autotag#readme"
}
